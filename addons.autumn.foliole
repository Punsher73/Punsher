  letter-spacing: 1px;
}
.mainContent-u_9PKf:hover::before {
  opacity: 1; 
  transform: scale(1, 1); 
}
.mainContent-u_9PKf::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  transition: all 0.3s;
  background-color: rgba(255, 255, 255, 0);
}
